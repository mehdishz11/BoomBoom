                                android:layout_height="8dp"
                                android:layout_alignParentLeft="true"
                                android:layout_marginLeft="17dp"
                                android:layout_marginTop="3dp"
                                android:alpha="0.5"
                                android:background="@drawable/circle_white"
                                android:rotation="60" />
                        </RelativeLayout>

                    </android.support.v7.widget.CardView>


